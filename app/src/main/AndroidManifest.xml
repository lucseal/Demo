<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.samuel.demo">

    <application
        android:name=".BasicApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.samuel.demo.home.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ocrTx.ui.TxOcrActivity" />

        <activity android:name=".ocrXf.ui.XfOcrActivity" />

        <activity android:name=".ocrBd.BdOcrMainActivity" />

        <activity
            android:name=".ocrBd.IDCardActivity"
            android:label="@string/idCardLabel" />

        <activity android:name=".coordinator.CoorActivity" />

    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>